{
    "name": "mjpramos/teste-pkg",
    "description": "Test Package",
    "keywords": [
        "mjpramos",
        "teste-pkg"
    ],
    "homepage": "https://github.com/mjpramos/teste-pkg",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "MÃ¡rio Ramos",
            "email": "mjpramos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mjpramos\\TestePkg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mjpramos\\TestePkg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
